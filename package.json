{
  "name": "diadust",
  "version": "0.0.1",
  "description": "",
  "main": "electron/index.js",
  "scripts": {
    "start": "electron ./electron/index.js",
    "client": "electron ./electron/index.html",
    "buildgo": "cd golang && export GOPATH=${PWD} && cd src/diadust && dep ensure -v && go build && go install",
    "webpack": "webpack --watch",
    "runpack": "electron ./dist/mac/diadust.app/Contents/Resources/app.asar",
    "pack": "electron-builder --mac --dir"
  },
  "build": {
    "files": [
      "**/*",
      "!bash",
      "!electron/components",
      "!electron/http",
      "!electron/model",
      "!golang/src${/*}",
      "!golang/pkg${/*}",
      "!node_modules${/*}",
      "!LICENSE",
      "!*.md",
      "!*.json",
      "!*.js"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/agreatfool/diadust.git"
  },
  "author": "Jonathan Dai",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/agreatfool/diadust/issues"
  },
  "homepage": "https://github.com/agreatfool/diadust#readme",
  "devDependencies": {
    "@types/file-type": "5.2.1",
    "@types/masonry-layout": "4.2.0",
    "@types/node": "8.10.17",
    "@types/node-fetch": "2.1.1",
    "@types/read-chunk": "2.1.0",
    "@types/uuid": "3.4.3",
    "autoprefixer": "6.6.0",
    "css-loader": "0.28.11",
    "electron": "2.0.2",
    "electron-builder": "20.15.1",
    "element-ui": "2.4.0",
    "file-loader": "1.1.11",
    "file-type": "8.0.0",
    "imagesloaded": "4.1.4",
    "masonry-layout": "4.2.1",
    "moment": "2.22.2",
    "postcss-loader": "1.3.3",
    "read-chunk": "2.1.0",
    "style-loader": "0.13.2",
    "ts-loader": "2.0.3",
    "typescript": "2.8.3",
    "url-loader": "0.5.8",
    "vue": "2.5.16",
    "vue-class-component": "6.2.0",
    "vue-i18n": "7.7.0",
    "vue-loader": "12.0.3",
    "vue-masonry": "0.11.1",
    "vue-mugen-scroll": "0.2.5",
    "vue-property-decorator": "6.1.0",
    "vue-router": "3.0.1",
    "vue-template-compiler": "2.5.16",
    "vuex": "3.0.1",
    "webpack": "2.5.0"
  },
  "dependencies": {
    "node-fetch": "2.1.2",
    "uuid": "3.2.1"
  }
}
